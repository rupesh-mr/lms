  <style>
    .colour-1 {
      background-image: initial;
      background-position-x: initial;
      background-position-y: initial;
      background-size: initial;
      background-repeat: initial;
      background-attachment: initial;
      background-origin: initial;
      background-clip: initial;
      background-color: rgb(44, 70, 122)
    }

    .container-foto>.image-box {
      position: relative;
      border-top-left-radius: 6px;
      border-top-right-radius: 6px;
    }

    .card-body article {
      position: absolute;
      width: 100%;
      top: -39%;
      margin-left: -15px;
    }

    .card-body h5 a {
      text-transform: uppercase;
      color: #fff !important;
      letter-spacing: normal;
      font-weight: bold;
    }

    .card-body .icon-sec {
      padding: 0px;
      border: 8px solid rgba(255, 255, 255, 0.16);
      overflow: hidden;
      width: 90px;
      height: 90px;
      position: absolute;
      left: 50%;
      margin-left: -40px;
      top: -45px;
      box-shadow: 0px 0px 12px 4px rgba(0, 0, 0, 0.1);
    }

    a {
      text-decoration: none !important;
    }

    c {
      color: #000;
    }
    .recommendation-title{
      text-align: center;
      margin-top: 20px;
      color: #89288E;
    }

    .book-shelf-container {
      background-size: cover;
      padding: 50px 0;
    }

    .book-shelf {
      display: flex;
      flex-wrap: wrap;
      justify-content: space-around;
      align-items: flex-start;
    }

    .book-card {
      cursor: pointer;
      transition: transform 0.2s;
      width: 200px;
      height: 350px;
      margin: 15px;
      display: flex;
      flex-direction: column;
      align-items: center;
      border: 1px solid #ccc;
      background-color: #fff;
      box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1);
      background-image: linear-gradient(135deg, #c4e0e5 0%, #4ca1af 100%);
      border-radius: 4px;
      overflow: hidden;
    }

    .book-card:hover {
      transform: scale(1.05);
    }

    .book-card img {
      width: 100%;
      height: auto;
      max-height: 200px;
      object-fit: contain;
      border-bottom: 1px solid #ccc;
      margin-bottom: 10px;
    }

    .book-card .card-body {
      height: 100px;
      width: 100%;
      padding: 10px;
      box-sizing: border-box;
      overflow-y: auto;
    }

    .book-title {
      font-size: 1rem;
      font-weight: bold;
      text-align: center;
      color: #333;
      margin: 10px 0 5px;
    }

    .book-author {
      font-size: 0.9rem;
      color: #555;
      text-align: center;
      margin: 0;
    }

    .shelf-divider {
      width: 100%;
      height: 10px;
      background-color: #8b5a2b;
      margin: 0 0 20px 0;
      border-radius: 5px;
    }
  </style>
</head>

